columncountquery="SELECT TABLE_SCHEMA, COUNT(*) FROM COLUMNS WHERE TABLE_SCHEMA LIKE \"$dbbasename%\" GROUP BY TABLE_SCHEMA ORDER BY TABLE_SCHEMA;"
tablecountquery="SELECT TABLE_SCHEMA, COUNT(*) FROM TABLES WHERE TABLE_SCHEMA LIKE \"$dbbasename%\" GROUP BY TABLE_SCHEMA ORDER BY TABLE_SCHEMA;"
tablelifetimequery="SELECT TABLE_NAME, COUNT(*) FROM TABLES WHERE TABLE_SCHEMA LIKE \"$dbbasename%\" GROUP BY TABLE_NAME ORDER BY count(*) DESC;"

